{
    "name": "soluzione-software/nova-translatable-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "astrotomic/laravel-translatable": "^11.1",
        "laravel/framework": "5.8.* | ^6.0 | ^7.0 | ^8.0",
        "laravel/nova": "~2.0 | ~3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SoluzioneSoftware\\Nova\\Fields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoluzioneSoftware\\Nova\\Fields\\TranslatableFieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
